{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 3000",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-app": "npm install && npm run build",
    "start": "node server.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@vueform/multiselect": "^2.6.11",
    "axios": "^1.12.2",
    "bootstrap": "^4.6.2",
    "bootstrap-icons-vue": "^1.11.3",
    "bootstrap-vue": "^2.23.1",
    "cors": "^2.8.5",
    "moment": "^2.30.1",
    "v-tooltip": "^2.1.3",
    "vue": "^2.7.16",
    "vue-bootstrap4-table": "^1.1.11",
    "vue-reactive-storage": "github:ropbla9/vue-reactive-storage",
    "vue-router": "^3.6.5",
    "vue-step-wizard": "^0.1.17",
    "vue-table-dynamic": "^0.4.10",
    "vuejs-smart-table": "^0.0.8",
    "vuex": "^3.6.2",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/eslint-parser": "^7.26.5",
    "@types/jquery": "^3.5.32",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.32.0",
    "vue-template-compiler": "^2.7.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-unused-vars": "off",
      "no-unused-components": "off",
      "vue/multi-word-component-names": [
        "error",
        {
          "ignores": [
            "Login",
            "Secure"
          ]
        }
      ]
    }
  },
  "proxy": "https://localhost:3000",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
